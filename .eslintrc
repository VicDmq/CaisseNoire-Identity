{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": [
    "prettier",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:flowtype/recommended",
    "plugin:testing-library/recommended",
    "plugin:jest-dom/recommended"
  ],
  "plugins": ["prettier", "react", "flowtype", "testing-library", "jest-dom"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["error"]
  },
  "settings": {
    "react": {
      "version": "detect",
      "flowVersion": "0.109.0"
    }
  }
}
